                <Path x:Name="path1" Height="12" Width="13" Stretch="Fill" Opacity="1" Data="M176,109 C176,109 206,109 206,109 C206,109 206,79 206,79 C206,79 176,109 176,109 z" Effect="{Binding Series.Effect}" Fill="{Binding Series.NegativeBrush}" StrokeLineJoin="Round" Margin="0" UseLayoutRounding="False" HorizontalAlignment="Right" VerticalAlignment="Bottom" Stroke="{Binding Series.ActualOutline}" StrokeThickness="{Binding Series.Thickness}"/>

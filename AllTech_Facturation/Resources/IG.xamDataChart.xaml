                <Path x:Name="path" Height="12" Width="13" Stretch="Fill" Opacity="1" Data="M176,109 C176,109 206,79 206,79 C206,79 176,79 176,79 C176,79 176,109 176,109 z" Effect="{Binding Series.Effect}" Fill="{Binding Series.ActualBrush}" HorizontalAlignment="Left" VerticalAlignment="Top" StrokeLineJoin="Round" UseLayoutRounding="False" Stroke="{Binding Series.ActualOutline}" StrokeThickness="{Binding Series.Thickness}"/>
                <Path x:Name="path1" Height="12" Width="13" Stretch="Fill" Opacity="1" Data="M176,109 C176,109 206,109 206,109 C206,109 206,79 206,79 C206,79 176,109 176,109 z" Effect="{Binding Series.Effect}" Fill="{Binding Series.NegativeBrush}" Margin="0" StrokeLineJoin="Round" UseLayoutRounding="False" HorizontalAlignment="Right" VerticalAlignment="Bottom" Stroke="{Binding Series.ActualOutline}" StrokeThickness="{Binding Series.Thickness}"/>

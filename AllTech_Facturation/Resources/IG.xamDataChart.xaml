    <!-- *********************************  ItemLegend style  ********************************* -->
    <Style TargetType="local:ItemLegend" x:Key="ItemLegendStyle">
        <Style.Setters>
            <Setter Property="Foreground" Value="{StaticResource ForegroundBrush}"/>
            <Setter Property="Background" Value="{StaticResource LegendBgBrush}"/>
            <Setter Property="BorderBrush" Value="{StaticResource GenericBorder}"/>
            <Setter Property="BorderThickness" Value="1"/>
            <Setter Property="Padding" Value="4"/>
            <Setter Property="Margin" Value="6"/>
            <Setter Property="Template">
                <Setter.Value>
                    <ControlTemplate TargetType="local:ItemLegend">
                        <Grid>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="15"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="15"/>
                            </Grid.ColumnDefinitions>
                            <Rectangle Fill="{StaticResource TransparentBrush}" Grid.ColumnSpan="3"/>
                            <Rectangle StrokeThickness="1" Grid.Column="2" Fill="{StaticResource ShadowBrushRight}"/>
                            <Rectangle StrokeThickness="1" Fill="{StaticResource ShadowBrushLeft}"/>
                            <Rectangle Stroke="{TemplateBinding BorderBrush}" Margin="0" Fill="{TemplateBinding Background}" StrokeThickness="{TemplateBinding BorderThickness}" Grid.Column="1"/>
                            <Border BorderThickness="{TemplateBinding BorderThickness}" CornerRadius="0" Grid.Column="1">
                                <Grid Margin="{TemplateBinding Padding}">
                                    <Grid.RowDefinitions>
                                        <RowDefinition Height="auto"/>
                                        <RowDefinition Height="*"/>
                                    </Grid.RowDefinitions>
                                    <ContentPresenter Grid.Row="0" Content="{TemplateBinding Content}"/>
                                    <ScrollViewer Grid.Row="1" HorizontalScrollBarVisibility="Auto" VerticalScrollBarVisibility="Auto" BorderThickness="0" Style="{StaticResource ScrollViewerStyle}">
                                        <ContentPresenter Grid.Row="1" x:Name="ContentPresenter"/>
                                    </ScrollViewer>
                                </Grid>
                            </Border>
                        </Grid>
                    </ControlTemplate>
                </Setter.Value>
            </Setter>
        </Style.Setters>
    </Style>
    <Style TargetType="local:ItemLegend" BasedOn="{StaticResource ItemLegendStyle}" />

    <!-- *********************************  ScaleLegendTemplate  ********************************* -->
    <DataTemplate x:Key="ScaleLegendTemplate">
        <Grid>
            <Grid.ColumnDefinitions>
                <ColumnDefinition/>
                <ColumnDefinition/>
            </Grid.ColumnDefinitions>
            <Polygon x:Name="LegendScale" Points="3 0, 5 0, 5 1, 0 1, 3 0" Stretch="Fill" Grid.Column="0" Margin="2,2,5,2" Fill="{StaticResource LegendScaleFill}"/>
            <TextBlock Text="Min" Grid.Column="1" VerticalAlignment="Top"/>
            <TextBlock Text="Max" Grid.Column="1" VerticalAlignment="Bottom"/>
        </Grid>
    </DataTemplate>

    <!-- *********************************  ScaleLegend style  ********************************* -->
    <Style TargetType="local:ScaleLegend" x:Key="ScaleLegendStyle">
        <Style.Setters>
            <Setter Property="Foreground" Value="{StaticResource ForegroundBrush}"/>
            <Setter Property="Background" Value="{StaticResource LegendBgBrush}"/>
            <Setter Property="BorderBrush" Value="{StaticResource GenericBorder}"/>
            <Setter Property="BorderThickness" Value="1"/>
            <Setter Property="Padding" Value="4"/>
            <Setter Property="Margin" Value="6"/>
            <Setter Property="MinHeight" Value="50"/>
            <Setter Property="Template">
                <Setter.Value>
                    <ControlTemplate TargetType="local:ScaleLegend">
                        <Grid>
                            <Grid.ColumnDefinitions>
                                <ColumnDefinition Width="15"/>
                                <ColumnDefinition Width="*"/>
                                <ColumnDefinition Width="15"/>
                            </Grid.ColumnDefinitions>
                            <Rectangle Fill="{StaticResource TransparentBrush}" Grid.ColumnSpan="3"/>
                            <Rectangle StrokeThickness="1" Grid.Column="2" Fill="{StaticResource ShadowBrushRight}"/>
                            <Rectangle StrokeThickness="1" Fill="{StaticResource ShadowBrushLeft}"/>
                            <Rectangle Stroke="{TemplateBinding BorderBrush}" Margin="0" Fill="{TemplateBinding Background}" StrokeThickness="{TemplateBinding BorderThickness}" Grid.Column="1"/>
                            <Border BorderThickness="{TemplateBinding BorderThickness}" CornerRadius="0" Grid.Column="1">
                                <Grid Margin="{TemplateBinding Padding}">
                                    <Grid.RowDefinitions>
                                        <RowDefinition Height="auto"/>
                                        <RowDefinition Height="*"/>
                                    </Grid.RowDefinitions>
                                    <ContentPresenter Grid.Row="0" Content="{TemplateBinding Content}"/>
                                    <ScrollViewer Grid.Row="1" HorizontalScrollBarVisibility="Auto" VerticalScrollBarVisibility="Auto" BorderThickness="0" Style="{StaticResource ScrollViewerStyle}">
                                        <ContentPresenter Grid.Row="1" x:Name="ContentPresenter">
                                            <ContentPresenter.Content>
                                                <Grid>
                                                    <Grid.ColumnDefinitions>
                                                        <ColumnDefinition/>
                                                        <ColumnDefinition/>
                                                    </Grid.ColumnDefinitions>
                                                    <Polygon x:Name="LegendScale" Points="3 0, 5 0, 5 1, 0 1, 3 0" Stretch="Fill" Grid.Column="0" Margin="2,2,5,2" Fill="{StaticResource LegendScaleFill2}"/>
                                                    <TextBlock x:Name="MinText" Text="Min" Grid.Column="1" VerticalAlignment="Top"/>
                                                    <TextBlock x:Name="MaxText" Text="Max" Grid.Column="1" VerticalAlignment="Bottom"/>
                                                </Grid>
                                            </ContentPresenter.Content>
                                        </ContentPresenter>
                                    </ScrollViewer>
                                </Grid>
                            </Border>
                        </Grid>
                    </ControlTemplate>
                </Setter.Value>
            </Setter>
        </Style.Setters>
    </Style>
    <Style TargetType="local:ScaleLegend" BasedOn="{StaticResource ScaleLegendStyle}" />
